---
import config from "@data/config.json";
---

<nav class="navbar bg-base-100 absolute top-0 left-0 z-20 bg-transparent">
  <ul class="menu menu-horizontal px-1 ml-auto">
    {
      config.pages.map((page) => (
        <li>
          <a href={page.url}>{page.name}</a>
        </li>
      ))
    }
  </ul>
</nav>
